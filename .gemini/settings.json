{
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "ui": {
    "showLineNumbers": true,
    "showCitations": true,
    "theme": "Dracula"
  },
  "tools": {
    "shell": {
      "autoApprove": [
        "bd",
        "cmake ",
        "git ",
        "hexy",
        "ls",
        "make ",
        "rm -f build/*",
        "vcpkg "
      ],
      "showColor": true
    },
    "autoAccept": true,
    "allowed": [
      "LSTool",
      "ReadFileTool",
      "WriteFileTool",
      "GrepTool",
      "GlobTool",
      "EditTool",
      "ReadManyFilesTool",
      "WebFetchTool",
      "WebSearchTool",
      "MemoryTool",
      "run_shell_command(git)",
      "run_shell_command(bd)",
      "run_shell_command(cmake)",
      "run_shell_command(ls)",
      "run_shell_command(make)",
      "run_shell_command(vcpkg)",
      "run_shell_command(rm -f build/)",
      "run_shell_command(cd)",
      "run_shell_command(ctest)",
      "run_shell_command(hexy)"
    ]
  }
}
